function extract(arr) {
    let biggestNumber = 0;
    let result = [];
    for (let i = 0; i < arr.length; i++) {
        if( arr[i] >= biggestNumber){
            biggestNumber = arr[i];
            result.push(biggestNumber);
        }
    }
    console.log(result.join('\n'));
}
extract([20,
    3,
    2,
    15,
    6,
    1]);